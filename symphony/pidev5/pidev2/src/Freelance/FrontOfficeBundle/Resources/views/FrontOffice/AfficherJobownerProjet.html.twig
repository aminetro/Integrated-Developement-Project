{%extends 'FreelanceFrontOfficeBundle::layout.html.twig' %}

 {% block content %} 
<h1> Mes Projets </h1>

<table class="table table-bordered">
    <thead>

        <th> Titre </th>
        <th> Categorie </th>
        <th> Duree </th>
        <th> Description </th>
        <th> Nom JOB Owner </th>
        <th> Nom Freelancer</th>
        <th> Modifier Mon PROJET</th>
        <th> Suprimer Mon PROJET</th>

    </thead>
{% for p in projet %}
    <tr>

        <td>{{p.titre_p}}</td>
        <td>{{p.categorie_p }}</td>
        <td>{{p.duree_p }}</td>
        <td>{{p.description_p }}</td>
        <td>{{p.owner_p }}</td>
        <td>{{p.pseudo_f }}</td>
        <td><a href="{{path('freelance_front_office_ModifierProjet',{id:p.id})}}">Modifier Projet</a></td>
            <td><a href="{{path('freelance_front_office_SuprimerProjet',{id:p.id})}}">Suprimer</a></td>

    </tr>
{% endfor %}
</table>

 {% endblock %}