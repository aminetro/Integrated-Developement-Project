<h1>Recherche d'un Jobowner</h1>
<form action="" method="POST">
    Recherche par pseudo du Jobowner:  <input type="text" name="search"/>
    <input type="submit" value="Recherche">
</form>

<table  class="table table-striped">
    <tr>
        <th> Nom </th>
        <th> Prenom </th>
        <th> Pays </th>
        <th> Ville </th>
        <th> Pseudo </th> 
       
        <th> Mail </th> 
        <th> societe </th> 
        <th> Supprimer </th> 
              
    </tr>
    
{% for jobowner in jobowner %}
    <tr>
<td>{{jobowner.nom }}</td>
<td>{{jobowner.prenom }}</td>
<td>{{jobowner.pays }}</td>
<td>{{jobowner.ville }}</td>
<td>{{jobowner.pseudo }}</td>

<td>{{jobowner.email }}</td>
<td>{{jobowner.societe_j }}</td>
<td><button type="button" class="btn btn-xs btn-default"><a href="{{ path('freelance_Back_deleteJobowner',{id:jobowner.id})}}">Supprimer</a></button> </td>

</tr>
{% endfor %}
